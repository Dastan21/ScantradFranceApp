{
  "expo": {
    "name": "ScantradFranceApp",
    "slug": "ScantradFranceApp",
    "privacy": "unlisted",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/icon.png",
    "splash": {
      "image": "./assets/img/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#12171E"
    },
    "androidStatusBar": {
      "hidden": true,
      "barStyle": "light-content"
    },
	"androidNavigationBar": {
		"visible": "immersive",
		"barStyle": "light-content"
	},
    "primaryColor": "#F05A28",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.dastan21.ScantradFranceApp"
    },
    "web": {
      "favicon": "./assets/img/favicon.png"
    },
    "packagerOpts": {
      "assetExts": [
        "ttf"
      ]
    }
  }
}
